<%- include("../partials/header") %>

<!--<div class="container">-->
    <div class="row">
        <h1 style="text-align: center">Edit <%= study_room.name %></h1>
        <div style="width: 30%; margin: 25px auto;">
            <form action="/study_rooms/<%= study_room._id %>/?_method=PUT" method="post">
                <div class="form-group">
                    <input class="form-control" type="text" name="study_room[name]" value="<%= study_room.name %>">
                </div>
                <div class="form-group">
                    <input class="form-control" type="text" name="study_room[image]" value="<%= study_room.image %>">
                </div>
                <div class="form-group">
                    <input class="form-control" type="text" name="study_room[description]" value="<%= study_room.description %>">
                </div>
                <div class="form-group">
                    <button class="btn btn-lg btn-primary btn-block">Submit!</button>
                </div>
            </form>
            <a href="/study_rooms">Go Back</a>
        </div>
    </div>
<!--</div>-->

<%- include("../partials/footer") %>
